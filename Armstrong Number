# Python program to determine whether the number is
# Armstrong number or not
# Function to check whether the given number is
# Armstrong number or not
from symbol import power

import order as order


def isArmstrong(x):
    n = order
    temp = x
    sum1 = 0
    while temp != 0:
        r = temp % 10
        sum1 = sum1 + power(r, n)
        temp = temp / 10

    # If condition satisfies
    return sum1 == x


# Driver Program
x = 153
print(isArmstrong(x))
